{
  "name": "mentalmodeler-js",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://jonnybomb.com/mentalmodeler",
  "dependencies": {
    "classnames": "^2.2.6",
    "html2canvas": "^1.0.0-alpha.12",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.5",
    "redux": "^4.0.0"
  },
  "scripts": {
    "start": "npm run build-css && run-p -ncr watch-css start-js",
    "start-js": "react-scripts start",
    "deploy": "run-s -n build-css build-js && rm -rf docs && mv build docs && git add docs && git commit -m \"task: deploy site\" && git push origin self-feedback-relationship",
    "build": "run-s -n build-css build-js && rm -rf docs && mv build docs",
    "build-js": "react-scripts build",
    "test": "run-s -n build-css test-js",
    "test-js": "react-scripts test --env=jsdom",
    "build-css": "node-less-chokidar src",
    "watch-css": "node-less-chokidar src --watch",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "node-less-chokidar": "^0.3.1",
    "npm-run-all": "^4.1.3"
  }
}
